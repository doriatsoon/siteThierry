  -moz-box-shadow: 0 3px 10px #666;
}

div.contenu4middle{
  background-color:white;
  margin-bottom:20px;
  width:100%;
  height:auto;
  text-align:justify;
  padding: 10px 10px 10px 10px;
  font-family:"HittheRoad", "Lucida Grande",Calibri, "Trebuchet MS", Helvetica, FreeSans, sans-serif;
  font-size: 14px;
  -webkit-box-shadow: 0 3px 10px #666;
  -moz-box-shadow: 0 3px 10px #666;
  -moz-border-radius: 8px;
  -webkit-border-radius: 8px;
   border-radius: 8px;
