#content {
  width: 1024px;
  height: 817px;
  margin: 0 auto;
  padding-top: 20px;
}


#content_right{
  width:200px;
  float:right;
}


#content_left{
  width:780px;
  float:left;
}


div.entete4middle{
  font-size:1.5em;
  font-weight:bold;
  background-color:white;
  -moz-border-radius: 8px;
  -webkit-border-radius: 8px;
